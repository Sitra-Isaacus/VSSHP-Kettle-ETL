    <sql>
-- tilaajat that are uniquely classifiable as internal or external

SELECT taso_3, taso_3_koodi, vsshp_boolean, vastaus_kuuluu_auria, aurialle_alkuhetki
FROM ${map_schema}.labra_tilaaja_tasot as tt
where vsshp_boolean is not null
and taso_3_koodi is not null and taso_3_koodi != ''
and not exists
(select * from ${map_schema}.labra_tilaaja_tasot as tt2
where tt2.taso_3_koodi = tt.taso_3_koodi and tt2.vsshp_boolean is not null and tt2.vsshp_boolean != tt.vsshp_boolean);
</sql>
