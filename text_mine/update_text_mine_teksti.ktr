    <sql>&#xa;SELECT&#xa;a.henkilotunnus,&#xa;p.potilasnumero,&#xa;CASE&#xa;    when t.teksti like &#x27;&#x3c;&#x25;&#x27; THEN &#x27;XML&#x27;&#x3a;&#x3a;text&#xa;    ELSE &#x27;suora&#x27;&#x3a;&#x3a;text&#xa;END AS teksti_muoto,&#xa;p.hoitotapahtuma_numero,&#xa;p.hoitotapahtuma_tunnus,&#xa;COALESCE&#x28;v.alkuhetki_pvm, al.tapahtuma_aika_alku&#x29; AS hoitotapahtuma_alkuhetki,&#xa;COALESCE&#x28;v.alkuhetki_pvm, al.tapahtuma_aika_alku, al.teksti_aika, p.paivityshetki_s&#x29; AS hoitotapahtuma_alkuhetki_arvio,&#xa;COALESCE&#x28;v.loppuhetki_pvm, al.tapahtuma_aika_loppu&#x29; AS hoitotapahtuma_loppuhetki,&#xa;p.potilaskertomus_numero,&#xa;p.toimipiste_koodi,&#xa;p.toimipiste_nimi,&#xa;y.yksikko_nimi,&#xa;p.nakyma_selite,&#xa;p.tila,&#xa;p.teksti_numero,&#xa;map.clean_up_text&#x28;t.teksti&#x29; as teksti,&#xa;p.paivityshetki_s AS kertomus_paivityshetki_s,&#xa;p.paivityshetki_ods AS kertomus_paivityshetki_ods,&#xa;t.paivityshetki_ods AS teksti_paivityshetki_ods,&#xa;al.teksti_aika,&#xa;t.paivityshetki_stage,&#xa;now&#x28;&#x29;&#xa;&#xa;FROM &#x24;&#x7b;source_schema&#x7d;.potilaskertomus p&#xa;     LEFT JOIN &#x24;&#x7b;source_schema&#x7d;.asiakas a ON p.potilasnumero&#x3a;&#x3a;text &#x3d; a.potilasnumero&#x3a;&#x3a;text&#xa;     LEFT JOIN &#x24;&#x7b;source_schema&#x7d;.mv_hoitotapahtuma v ON v.hoitotapahtuma_numero&#x3a;&#x3a;text &#x3d; p.hoitotapahtuma_numero&#x3a;&#x3a;text AND v.hoitotapahtuma_tunnus&#x3a;&#x3a;text &#x3d; p.hoitotapahtuma_tunnus&#x3a;&#x3a;text&#xa;     LEFT JOIN &#x24;&#x7b;target_schema&#x7d;.aikaleimat al ON p.teksti_numero&#x3a;&#x3a;text &#x3d; al.teksti_numero&#x3a;&#x3a;text&#xa;     INNER JOIN &#x24;&#x7b;source_schema&#x7d;.potilaskertomus_teksti t ON p.teksti_numero&#x3a;&#x3a;text &#x3d; t.teksti_numero&#x3a;&#x3a;text&#xa;	LEFT JOIN &#x24;&#x7b;source_schema&#x7d;.yksikko as y on p.toimipiste_koodi &#x3d; y.yksikko_koodi or &#x28;p.toimipiste_nimi &#x3d; y.yksikko_koodi and p.toimipiste_koodi &#x21;&#x3d; y.yksikko_koodi&#x29;&#