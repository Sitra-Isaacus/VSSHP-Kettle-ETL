      <add_date>N</add_date>
      <add_time>N</add_time>
      <insertScript>Y</insertScript>
      <script>#!/bin/bash

# count .gz files in source folder
files_gz=$(ls -U ${source_folder}*.gz 2> /dev/null | wc -l)


# if .gz files exist
if [ $files_gz != 0 ]

then 

	# copy .gz files from source to temp folder
	cp ${source_folder}*.gz ${temp_folder}
	
	# unzip files in temp folder
	gunzip ${temp_folder}*.gz
	
	# remove bom
	perl -p -i -e 's/\xEF\xBB\xBF//' ${temp_folder}*.xml

fi




# count .xml files in source folder
files_xml=$(ls -U ${source_folder}*.xml 2> /dev/null | wc -l)

# if .xml files exist
if [  $files_xml != 0 ]

then 

	# copy .xml files from source to temp folder
	cp ${source_folder}*.xml ${temp_folder}
	
	# remove bom
	perl -p -i -e 's/\xEF\xBB\xBF//' ${temp_folder}*.xml

fi







</script>
