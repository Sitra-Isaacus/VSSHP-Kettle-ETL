    <entry>
      <name>SQL - Refresh Uraods materialized views</name>
      <description />
      <type>SQL</type>
      <sql>
----------------------------------------------------------
update stage_uraods.cdc_time
set current_load = now()
where process_name = 'mv_palvelu';
refresh materialized view stage_uraods.mv_palvelu;
update stage_uraods.cdc_time
set last_load = current_load, current_load = null
where process_name = 'mv_palvelu';
----------------------------------------------------------
update stage_uraods.cdc_time
set current_load = now()
where process_name = 'mv_palvelu_diagnoosi';
refresh materialized view stage_uraods.mv_palvelu_diagnoosi;
update stage_uraods.cdc_time
set last_load = current_load, current_load = null
where process_name = 'mv_palvelu_diagnoosi';
----------------------------------------------------------
update stage_uraods.cdc_time
set current_load = now()
where process_name = 'mv_diagnoosi';
refresh materialized view stage_uraods.mv_diagnoosi;
update stage_uraods.cdc_time
set last_load = current_load, current_load = null
where process_name = 'mv_diagnoosi';
----------------------------------------------------------
update stage_uraods.cdc_time
set current_load = now()
where process_name = 'mv_laakemaarays';
refresh materialized view stage_uraods.mv_laakemaarays;
update stage_uraods.cdc_time
set last_load = current_load, current_load = null
where process_name = 'mv_laakemaarays';
