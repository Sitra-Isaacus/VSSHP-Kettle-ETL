, WRK_Name
, MEAS_Date
FROM SUBMEASURE
WHERE ? &lt;= coalesce(dateadd(hour, 1, SUBMEAS_ModDate), dateadd(hour, 1, SUBMEAS_GenDate), SUBMEAS_LocalDate)
AND coalesce(dateadd(hour, 1, SUBMEAS_ModDate), dateadd(hour, 1, SUBMEAS_GenDate), SUBMEAS_LocalDate) &lt; ?
