    </partitioning>
    <connection>PostgreSQL gradient</connection>
    <sql>--&#xa;-- Read those entries that have changed at the stage since the last update&#xa;--&#xa;SELECT &#xa;	&#x3f; AS lahde_koodi_id,&#xa;	asiakas.yhd_viite,&#xa;	syntymaaika_pvm,&#xa;	kuolinaika_pvm,&#xa;	lahde_paivityshetki,&#xa;    CAST&#x28;&#x3f; AS TIMESTAMP&#x29; AS paivityshetki&#xa;FROM &#x24;&#x7b;stage_schema&#x7d;.asiakas&#xa;JOIN &#x28;&#xa;--&#xa;-- Filter those entries that have changed since the last update and use only&#xa;-- the latest information, grouped by &#x27;yhd_viite&#x27; &#x28;which is&#xa;-- the combined &#x27;potilasnumero&#x27;&#x29;&#xa;--&#xa;SELECT&#xa;	yhd_viite,&#xa;	max&#x28;paivityshetki_s&#x29; AS lahde_paivityshetki&#xa;FROM &#x24;&#x7b;stage_schema&#x7d;.asiakas&#xa;WHERE &#xa;-- Last update in main.potilas &#x3c;&#x3d; last update in stage_uraods.asiakas&#xa; &#x28;SELECT COALESCE&#x28;MAX&#x28;paivityshetki&#x29;, &#x27;1970-01-01 00&#x3a;00&#x3a;00&#x27;&#x29; &#xa;  AS last_update FROM &#x24;&#x7b;main_schema&#x7d;.potilas&#x29; &#x3c;&#x3d; paivityshetki_ods&#xa;GROUP BY yhd_viite &#x29; AS T&#xa;-- Match by &#x27;potilasnumero&#x27; and the latest update time&#xa;ON asiakas.potilasnumero &#x3d; T.yhd_viite &#xa;AND asiakas.paivityshetki_s &#x3d; lahde_paivityshetki&#x3b;&#xa;</sql>
    <limit>0</limit>
    <lookup>Read stage update time</lookup>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
    <cluster_schema/>
