    <connection>PostgreSQL parametrized 1</connection>
    <sql>&#xa;select t1.qpatianswer_id, t1.tablenumber, t1.tablename, t1.value as variable,&#xa;t2.value as value&#xa;&#xa;-- t1 on otsikko sarakkeessa 0, t2 on arvo &#x28;&#x3d;muu kuin otsikko&#x29; sarakkeessa 1&#xa;&#xa;from &#x24;&#x7b;source_schema&#x7d;.statementtablecelldata as t1&#xa;inner join &#x24;&#x7b;source_schema&#x7d;.statementtablecelldata as t2 on &#x28;t1.qpatianswer_id &#x3d; t2.qpatianswer_id&#xa;and t1.tablenumber &#x3d; t2.tablenumber and t1.tablename &#x3d; t2.tablename and t1.col&#x3d;0 and t2.col &#x3d; 1&#xa;and t1.row &#x3d; t2.row&#x29;&#xa;-- include only samples that are being processed right now&#xa;inner join &#x24;&#x7b;source_schema&#x7d;.zz_sample_order_answer as a on t1.qpatianswer_id &#x3d; a.qpatianswer_id&#xa;&#xa;where t1.tablename &#x7e;&#x2a; &#x27;&#x5c;.tbl&#x24;&#x27; -- tablename ends with &#x27;.tbl&#x27;&#xa;and t1.tablename &#x21;&#x3d; &#x27;OBDUKTIO MIKROSKOOPPISET N&#xc4;YTTEET.tbl&#x27; --. these two tables have only one column, not two, and need to be processed separately&#xa;and t1.tablename &#x21;&#x3d; &#x27;AIVO-OBDUKTIO MIKROSKOOPPISET N&#xc4;YTTEET.tbl&#x27;&#xa;and t1.value is not null -- there is a value for the variable name&#xa;and t2.value is not null -- there is a value for the variable value, too&#xa;and t1.value &#x21;&#x3d; &#x27;&#x27;&#xa;and t2.value &#x21;&#x3d; &#x27;&#x27;&#xa;&#xa;and not exists -- taulukossa ei ole muita sarakkeita kuin 0 ja 1&#xa;&#x28;select &#x2a; from &#x24;&#x7b;source_schema&#x7d;.statementtablecelldata as t3&#xa;where t3.qpatianswer_id &#x3d; t1.qpatianswer_id&#xa;and t3.tablenumber &#x3d; t1.tablenumber and t3.tablename &#x3d; t1.tablename and t3.col&#x3e;1&#x29;&#x3b;&#xa;&#xa;&#xa;&#xa;</sql>
